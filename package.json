{
  "scripts": {
    "rm": "rm -rf dist",
    "dev": "NODE_ENV=dev nodemon build/bin/start.js",
    "compile": "yarn run rm && NODE_ENV webpack --progress --colors --config build/bin/compile",
    "compile:prd": "yarn run rm && NODE_ENV=production webpack --progress --colors --config build/bin/compile",
    "deploy": "NODE_ENV=production webpack --progress --colors --config build/config/webpack.base.config",
    "e2e": "cross-env NODE_ENV=test nodemon ./test/e2e/runner.js",
    "ssr:dev": "nodemon ./ssr-server.js",
    "ssr:start": "cross-env NODE_ENV=production nodemon ssr-server.js",
    "ssr:build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/config/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint --max-warnings 10 --ext .js,.vue .",
    "lint:fix": "yarn run lint -- --fix",
    "lint:styl": "node build/bin/stylint.js",
    "push": "zsh ./build/cli/push.sh",
    "synch": "zsh ./build/cli/synch.sh"
  },
